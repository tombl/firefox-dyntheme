{
  "description": "Firefox dynamic theme reloader",
  "homepage_url": "https://github.com/tombl/firefox-dyntheme",
  "manifest_version": 2,
  "name": "dyntheme",
  "permissions": [
    "nativeMessaging",
    "theme"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "dyntheme@tombl.dev"
    }
  },
  "version": "1.0.2"
}
